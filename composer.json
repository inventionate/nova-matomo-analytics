{
    "name": "rocramer/nova-matomo-analytics",
    "authors": [
        {
            "name": "Robin Cramer",
            "email": "mail@robincramer.de",
            "homepage": "https://www.robincramer.de",
            "role": "Developer"
        }    ],
    "description": "Matomo Analytics cards for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Rocramer\\MatomoAnalytics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rocramer\\MatomoAnalytics\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
    "require-dev": {
        "laravel/nova": "^4.2"
    }
}
